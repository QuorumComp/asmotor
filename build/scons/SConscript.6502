Import("env")
Import("libxasm")
Import("libutil")

b = "build_6502"

localEnv = env.Clone()

localEnv.VariantDir(b, "../../xasm/6502")
motor6502 = localEnv.Program(b + "/motor6502", LIBS = [libxasm, libutil, "m"], source = [
	b + "/locerror.c",
	b + "/loclexer.c",
	b + "/locopt.c",
	b + "/locparse.c",
	b + "/locsym.c",
	b + "/main.c"
	])

Return("motor6502")
