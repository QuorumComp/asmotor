Import("env")
Import("libxasm")
Import("libutil")

b = "build_680x0"

localEnv = env.Clone()

localEnv.VariantDir(b, "../../xasm/680x0", duplicate = 0)
motor68k = localEnv.Program(b + "/motor68k", LIBS = [libxasm, libutil, "m"], source = [
	b + "/locerror.c",
	b + "/loclexer.c",
	b + "/locopt.c",
	b + "/locparse.c",
	b + "/locsym.c",
	b + "/main.c"
	])

Return("motor68k")
