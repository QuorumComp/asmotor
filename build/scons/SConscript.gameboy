Import("env")
Import("libxasm")
Import("libutil")

b = "build_gameboy"

localEnv = env.Clone()

localEnv.VariantDir(b, "../../xasm/gameboy")
motorgb = localEnv.Program(b + "/motorgb", LIBS = [libxasm, libutil, "m"], source = [
	b + "/locerror.c",
	b + "/loclexer.c",
	b + "/locopt.c",
	b + "/locparse.c",
	b + "/locsym.c",
	b + "/main.c"
	])

Return("motorgb")
