Import("env")
Import("libxasm")
Import("libutil")

b = "build_mips"

localEnv = env.Clone()

localEnv.VariantDir(b, "../../xasm/mips", duplicate = 0)
motormips = localEnv.Program(b + "/motormips", LIBS = [libxasm, libutil, "m"], source = [
	b + "/locerror.c",
	b + "/loclexer.c",
	b + "/locopt.c",
	b + "/locparse.c",
	b + "/locsym.c",
	b + "/main.c"
	])

Return("motormips")
