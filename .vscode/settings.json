{
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "files.associations": {
    "*.rc8": "asm.z80",
    "exec.h": "c",
    "xlink.h": "c",
    "limits": "c",
    "type_traits": "c",
    "options.h": "c",
    "project.h": "c",
    "expression.h": "c",
    "dependency.h": "c",
    "strbuf.h": "c",
    "rc8_errors.h": "c",
    "strcoll.h": "c"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/CMakeFiles": true,
    "**/cmake_install.cmake": true,
    "**/*.a": true
  },
  "cmake.configureOnOpen": true,
  "[asm.z80]": {
      "editor.tabSize": 8,
      "editor.insertSpaces": false
  },
  "C_Cpp.errorSquiggles": "Enabled"
}