    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug xlink",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cmake/debug/xlink/xlink",
            "args": ["-tb", "-oamigaobj.68k.bin", "amigaobj.68k.obj"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/test/680x0",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb"
        },
        {
            "name": "Debug motorz80",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cmake/debug/xasm/z80/motorz80",
            "args": ["-d.d/vectors.Td", "-mcz", "-ms1", "-mu1", "-z0", "-ovectors.obj", "vectors.asm"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/../mastersystem-test",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb"
        },
        {
            "name": "Debug motor68k",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cmake/debug/xasm/680x0/motor68k",
            "args": ["test.68k"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/test/680x0",
            "environment": [],
            "externalConsole": true,
            "MIMode": "lldb"
        },
        {
            "name": "Debug motordcpu",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cmake/debug/xasm/dcpu-16/motordcpu16",
            "args": ["-fv", "-otest.vh", "test.asm"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/test/dcpu-16",
            "environment": [],
            "externalConsole": true,
            "MIMode": "lldb"
        }
    ]
}